{
  "openapi": "3.1.0",
  "info": {
    "title": "DCF Agent API",
    "version": "1.0.0",
    "description": "Aggregated DCF valuation API for AI agents."
  },
  "paths": {
    "/healthz": {
      "get": {
        "responses": {
          "200": {
            "description": "Service health"
          }
        }
      }
    },
    "/v1/valuation": {
      "post": {
        "parameters": [
          {
            "in": "header",
            "name": "x-agent-key",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response"
          },
          "400": {
            "description": "Invalid request"
          },
          "401": {
            "description": "Unauthorized"
          },
          "404": {
            "description": "Symbol not found"
          },
          "422": {
            "description": "Invalid override values"
          },
          "500": {
            "description": "Internal error"
          }
        }
      }
    }
  }
}
